{
    "name": "RedBear Duo",
    "based_on": "https://github.com/redbear/Duo",
    "map": {
        "digital": {
            "pins": {
                "D0":  0,  "D1":  1,  "D2":  2,  "D3":  3,  "D4":  4,
                "D5":  5,  "D6":  6,  "D7":  7,
                                                 "D8":  10, "D9":  11,
                "D10": 12, "D11": 13, "D12": 14, "D13": 15, "D14": 16,
                "D15": 17, "D16": 18, "D17": 19
            },
            "ops": [ "dr", "dw" ]
        },
        "analog": {
            "pins": {
                "A0": 10, "A1": 11, "A2": 12, "A3": 13, "A4": 14, "A5": 15,
                "A6": 16, "A7": 17
            },
            "ops": [ "dr", "dw", "ar" ],
            "arRange": [0, 1023]
        },
        "pwm": {
            "pins": [
                "D0", "D1", "D2", "D3", "D4",
                "D8", "D9", "D12", "D13", "D14", "D15", "D16", "D17"
            ],
            "ops": [ "aw" ],
            "awRange": [0, 255]
        },
        "virtual":  {
            "pinsRange": [ 0, 127 ],
            "ops": [ "vr", "vw" ]
        }
    }
}
