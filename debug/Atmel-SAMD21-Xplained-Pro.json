{
    "name": "Atmel SAMD21 Xplained Pro",
    "map": {
        "digital": {
            "pins": {
                "D0":  0,  "D1":  1,  "D2":  2,  "D3":  3,  "D4":  4,
                "D5":  5,  "D6":  6,  "D7":  7,  "D8":  8,  "D9":  9,
                "D10": 10, "D11": 11, "D12": 12, "D13": 13, "D14": 14,
                "D15": 15, "D16": 16, "D17": 17, "D18": 18, "D19": 19,
                "D20": 20, "D21": 21
            },
            "ops": [ "dr", "dw" ]
        },
        "analog": {
            "pins": {
                "A0": 22, "A1": 23, "A2": 24, "A3": 25, "A4": 26, "A5": 27
            },
            "ops": [ "dr", "dw", "ar" ],
            "arRange":[0, 1023]
        },
        "pwm": {
            "pins": [
                "D16", "D17", "D18", "D19", "D20", "D21"
            ],
            "ops": [ "aw" ],
            "awRange":[0, 255]
        },
        "virtual":  {
            "pinsRange": [ 0, 31 ],
            "ops": [ "vr", "vw" ]
        }
    }
}
